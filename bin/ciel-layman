#!/bin/bash

###################################
# Detect dependencies
###################################
for MYDEP in jq; do
    if [[ "$(which $MYDEP)" != "/"* ]]; then
        echo "[ERROR] Dependency $MYDEP is not satisfied. Aborting."
        exit 1
    fi
done
echo "[INFO] Dependencies are satisfied. Good."

